<?xml version="1.0" encoding="utf-8"?>
<manifest package="is.reon.datahack2018"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="DataHackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.DataConnectActivity"
            android:screenOrientation="portrait"/>

        <meta-data android:name="me.digi.sdk.Contracts" android:resource="<>" />
        <meta-data android:name="me.digi.sdk.AppId" android:value="<>" />
        <meta-data android:name="me.digi.sdk.AppName" android:value="<>" />
        <meta-data android:name="me.digi.sdk.Keys" android:value="<>.p12" />
        <meta-data android:name="me.digi.sdk.KeysPassphrase" android:value="<>" />

    </application>

</manifest>